Algoritmo "questao8"

Var
Capital, Juros, Meses, F:real


Inicio
escreval("Insira o capital")
leia(Capital)
escreval("Insira o valor dos juros")
leia(juros)
escreval("Insira a quantidade de meses de aplicacao")
leia(Meses)



F <- Capital * (1+juros/100)Meses;

escreval("O montante arrecadado equivale a R$        ", F)


Fimalgoritmo
